{
  "displayName": "Homebridge SwitchBot",
  "name": "@switchbot/homebridge-switchbot",
  "version": "2.2.2",
  "description": "The [Homebridge](https://homebridge.io) SwitchBot plugin allows you to access your [SwitchBot](https://www.switch-bot.com) device(s) from HomeKit.",
  "author": "SwitchBot <support@wondertechlabs.com> (https://github.com/SwitchBot)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/OpenWonderLabs/homebridge-switchbot.git"
  },
  "bugs": {
    "url": "https://github.com/OpenWonderLabs/homebridge-switchbot/issues"
  },
  "engines": {
    "homebridge": "^1.6.0 || ^2.0.0-beta.0",
    "node": "^14.21.1 || ^16.18.1 || ^18.12.1"
  },
  "main": "dist/index.js",
  "scripts": {
    "check": "npm install && npm outdated",
    "update": "ncu -u && npm update && npm install",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build",
    "postpublish": "npm run clean",
    "clean": "rimraf ./dist",
    "test": "eslint src/**.ts"
  },
  "funding": [
    {
      "type": "Paypal",
      "url": "https://paypal.me/donavanbecker"
    },
    {
      "type": "GitHub",
      "url": "https://github.com/sponsors/donavanbecker"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "homebridge-plugin",
    "switchbot",
    "lock",
    "humidifier",
    "humidity",
    "temperature",
    "bot",
    "curtains",
    "windows",
    "ble",
    "ir"
  ],
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^0.0.19",
    "fakegato-history": "^0.6.3",
    "super-stringify": "^1.0.0",
    "async-mqtt": "^2.6.3",
    "crypto-js": "^4.1.1",
    "rxjs": "^7.6.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.11",
    "@types/crypto-js": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "homebridge": "^1.6.0",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.5.1",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "optionalDependencies": {
    "node-switchbot": "^1.7.0",
    "@abandonware/noble": "^1.9.2-15",
    "@abandonware/bluetooth-hci-socket": "^0.5.3-8"
  }
}
